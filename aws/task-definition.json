{
  "family": "notegen-ai-api-staging",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::ACCOUNT_ID:role/notegen-ai-api-staging-execution-role",
  "taskRoleArn": "arn:aws:iam::ACCOUNT_ID:role/notegen-ai-api-staging-task-role",
  "containerDefinitions": [
    {
      "name": "notegen-ai-api",
      "image": "ACCOUNT_ID.dkr.ecr.ca-central-1.amazonaws.com/notegen-ai-api:latest",
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "APP_NAME",
          "value": "notegen-ai-api"
        },
        {
          "name": "ENVIRONMENT",
          "value": "staging"
        },
        {
          "name": "LOG_LEVEL",
          "value": "INFO"
        },
        {
          "name": "DEBUG",
          "value": "false"
        },
        {
          "name": "CORS_ORIGINS",
          "value": "http://localhost:3000,http://localhost:8000"
        },
        {
          "name": "OPENSEARCH_INDEX",
          "value": "medical-conversations"
        },
        {
          "name": "NEO4J_USER",
          "value": "neo4j"
        },
        {
          "name": "NEO4J_DATABASE",
          "value": "neo4j"
        },
        {
          "name": "AZURE_OPENAI_DEPLOYMENT_NAME",
          "value": "gpt-4o"
        },
        {
          "name": "AZURE_OPENAI_API_VERSION",
          "value": "2024-05-01-preview"
        },
        {
          "name": "AZURE_OPENAI_MODEL",
          "value": "gpt-4o"
        },
        {
          "name": "AZURE_OPENAI_EMBEDDING_DEPLOYMENT_NAME",
          "value": "text-embedding-ada-002"
        },
        {
          "name": "AZURE_OPENAI_EMBEDDING_MODEL",
          "value": "text-embedding-ada-002"
        },
        {
          "name": "LANGFUSE_HOST",
          "value": "https://us.cloud.langfuse.com"
        },
        {
          "name": "PYTHONPATH",
          "value": "/app"
        }
      ],
      "secrets": [
        {
          "name": "NEO4J_URI",
          "valueFrom": "arn:aws:ssm:ca-central-1:ACCOUNT_ID:parameter/notegen-ai-api/staging/neo4j/uri"
        },
        {
          "name": "OPENSEARCH_ENDPOINT",
          "valueFrom": "arn:aws:ssm:ca-central-1:ACCOUNT_ID:parameter/notegen-ai-api/staging/opensearch/endpoint"
        },
        {
          "name": "AZURE_OPENAI_ENDPOINT",
          "valueFrom": "arn:aws:ssm:ca-central-1:ACCOUNT_ID:parameter/notegen-ai-api/staging/azure-openai/endpoint"
        },
        {
          "name": "AZURE_OPENAI_EMBEDDING_ENDPOINT",
          "valueFrom": "arn:aws:ssm:ca-central-1:ACCOUNT_ID:parameter/notegen-ai-api/staging/azure-openai/embedding-endpoint"
        },
        {
          "name": "NOTEGEN_API_BASE_URL",
          "valueFrom": "arn:aws:ssm:ca-central-1:ACCOUNT_ID:parameter/notegen-ai-api/staging/notegen-api/base-url"
        },
        {
          "name": "NEO4J_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:ca-central-1:ACCOUNT_ID:secret:notegen-ai-api-staging-neo4j-password-o4xkzI"
        },      
        {
          "name": "AZURE_OPENAI_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ca-central-1:ACCOUNT_ID:secret:notegen-ai-api-staging-azure-openai-api-key-gMSmxM"
        },
        {
          "name": "AZURE_OPENAI_EMBEDDING_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ca-central-1:ACCOUNT_ID:secret:notegen-ai-api-staging-azure-openai-embedding-api-key-nybEXj"
        },
        {
          "name": "LANGFUSE_SECRET_KEY",
          "valueFrom": "arn:aws:secretsmanager:ca-central-1:ACCOUNT_ID:secret:notegen-ai-api-staging-langfuse-secret-key-R65eAa"
        },
        {
          "name": "LANGFUSE_PUBLIC_KEY",
          "valueFrom": "arn:aws:secretsmanager:ca-central-1:ACCOUNT_ID:secret:notegen-ai-api-staging-langfuse-public-key-ipG1UU"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/notegen-ai-api-staging",
          "awslogs-region": "ca-central-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8000/health/ || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 120
      }
    }
  ]
} 